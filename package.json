{
  "name": "parcel-transformer-minify-literals",
  "version": "1.0.10",
  "private": false,
  "description": "Minify html, css literals for ParcelJS",
  "keywords": [
    "parcel minify literals",
    "html literal",
    "css literal",
    "parcel lit minify",
    "minify literals"
  ],
  "homepage": "https://github.com/kaifaty/parcel-transformer-minify-literals",
  "bugs": {
    "url": "https://github.com/kaifatyparcel-transformer-minify-literals/issues"
  },
  "license": "MIT",
  "author": "kaifaty",
